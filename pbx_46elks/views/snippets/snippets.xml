<?xml version="1.0" encoding="utf-8"?>

<odoo>   

    <template id="custom_snippets" inherit_id="website.snippets" name="Custom Snippets">

        <xpath expr="//div[@id='snippet_structure']" position="before">
            
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">46elks</div>
                <div class="o_panel_body">

                    <t t-snippet="pbx_46elks.webrtc_page" t-thumbnail="/website/static/src/img/snippets_thumbs/s_banner.svg">
                        <keywords>hero, jumbotron</keywords>
                    </t>

                </div>
            </div>
        </xpath>

    </template>

</odoo>